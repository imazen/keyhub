@model KeyHub.Web.ViewModels.Transaction.TransactionDetailsViewModel

@{
    ViewBag.Title = "Complete";
}

<h2>License claim complete</h2>

<fieldset>
    <legend>TransactionViewModel</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Transaction.TransactionId)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Transaction.TransactionId)
    </div>
</fieldset>

@Html.Partial("DetailsPartial", Model) 

<br />

<h3>Registered licenses</h3>
